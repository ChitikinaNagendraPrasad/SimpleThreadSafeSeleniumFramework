<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!--<suite name="BankingDemoSuite" parallel="methods" thread-count="3">-->
<suite name="BankingDemoSuite" parallel="none" thread-count="4">
	<!-- Suite level parameters -->
	<parameter name="browser" value="firefox" />
	<parameter name="base.url" value="https://www.automationexercise.com/" />
	<parameter name="implicit.wait.seconds" value="5" />
	<parameter name="explicit.wait.seconds" value="10" />

	<listeners>
		<listener
			class-name="com.example.framework.listeners.ExtentTestNGListener" />
	</listeners>

	<test name="MyTestCases">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.example.framework.tests.MyTest" />
		</classes>
	</test>
</suite>
